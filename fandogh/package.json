{
  "name": "fandogh",
  "description": "cli tool to interact with fandogh paas",
  "version": "0.0.1",
  "author": "Sorena Sarabadani @Anonycoders",
  "bin": {
    "fandogh": "./bin/run"
  },
  "bugs": "https://github.com/fandoghpaas/fandogh-cli-node/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "tslib": "^1",
    "inquirer": "^7.0.0",
    "ora": "^3.4.0",
    "node-notifier": "^6.0.0",
    "fs-extra": "^8.1.0",
    "js-yaml": "^3.13.1",
    "active-win": "^5.1.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/tslint": "^3",
    "@types/node": "^10",
    "globby": "^10",
    "ts-node": "^8",
    "tslint": "^5",
    "typescript": "^3.3",
    "@types/inquirer": "^6.0.0",
    "@types/node-notifier": "^5.4.0",
    "@types/js-yaml": "^3.12.1",
    "@types/fs-extra": "^8.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/fandoghpaas/fandogh-cli-node",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "fandogh",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "fandoghpaas/fandogh-cli-node",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p . -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
